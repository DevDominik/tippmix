<Page x:Class="Tippmixx.OrganizerPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Tippmixx"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="OrganizerPage">

    <Grid>
        <StackPanel>
            <TextBox x:Name="tbOrganizerSearch" TextChanged="tbOrganizerSearch_TextChanged"></TextBox>
        <DataGrid x:Name="dtgEvents" SelectionChanged="dtgEvents_SelectionChanged"></DataGrid>
            
        </StackPanel>
    </Grid>
</Page>
